{
  "apis" : {
    "getCustomCredits" : {
      "200" : {
        "userId" : "#/generators/gamification_userId",
        "startTime" : "#/generators/startTime",
        "endTime" : "#/generators/endTime"
      }
    },
    "getBadges" : {
      "200" : {
        "userId" : "#/generators/gamification_userId",
        "startTime" : "#/generators/startTime",
        "endTime" : "#/generators/endTime"
      }
    },
    "getUserGamificationInfo" : {
      "200" : {
        "userId" : "#/generators/gamification_userId"
      }
    },
    "getScoreBoard" : {
      "200" : {
        "userId" : "#/generators/gamification_userId"
      }
    }
  },
  "generators" : {
    "gamification_userId" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "portal.CommunityUser.myFollowedUsers",
      "dataPath" : "$.response.body:$.data[*].id"
    } ],
    "startTime" : [ {
      "type" : "remote",
      "generatorMethod" : "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
      "inputs" : {
        "timeLine" : "past",
        "format" : "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"
      }
    } ],
    "endTime" : [ {
      "type" : "remote",
      "generatorMethod" : "applicationDriver.rpc.desk.DynamicDataProvider.getDateTime",
      "inputs" : {
        "timeLine" : "future",
        "format" : "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"
      }
    } ]
  }
}