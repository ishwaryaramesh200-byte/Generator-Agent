        "categoryId" : "#/generators/getCategoryId"
      }
    },
    "getCategoryByPermalink" : {
      "422" : {
        "permalink" : "#/generators/getCategoryByPermalink_permalink"
      },
      "200" : {
        "permalink" : "#/generators/getCategoryByPermalink_permalink"
      }
    },
    "getCategoryLogo" : {
      "200" : {
        "categoryId" : "#/generators/getCategoryId"
      }
    },
    "listAllCommunityCategoryFollowers" : {
      "200" : {
        "categoryId" : "#/generators/getCategoryId"
      }
    }
  },
  "generators" : {
    "getCategoryId" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "portal.Category.listAllCategory",
      "dataPath" : "$.response.body:$.data[*].id"
    } ],
    "getCategoryByPermalink_permalink" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "portal.Category.getACategory",
      "dataPath" : "$.response.body:$.permalink"
