{
  "apis": {
    "getAttachments": {
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      },
      "204": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      }
    },
    "viewAccountAttachment": {
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id",
        "attachmentId": "#/generators/account_attachment_id"
      }
    },
    "createAttachment": {
      "422": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      },
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      }
    },
    "deleteAttachment": {
      "404": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id",
        "attachmentId": "#/generators/account_attachment_id"
      },
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id",
        "attachmentId": "#/generators/account_attachment_id"
      }
    }
  },
  "generators": {
    "account_attachment_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.AccountAttachment.createAttachment",
        "dataPath": "$.response.body:$.id" ,
        "params" : {
          "accountId" : "$accounts.response.body:$.id"
        }
      }
    ]
  }
}