{
	"apis": {
		"getUsersUnderLabel": {
			"422": {
				"labelId": "#/generators/label_id"
			},
			"200": {
				"labelId": "#/generators/label_id"
			}
		},
		"addLabel": {
			"422": {
				"helpCenterId": "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id",
				"logoId": "#/generators/logo_id"
			},
			"200": {
				"helpCenterId": "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id",
				"logoId": "#/generators/logo_id"
			}
		},
		"getLabels": {
			"422": {
				"helpCenterId": "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id"
			},
			"200": {
				"helpCenterId": "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id"
			}
		},
		"getLabel": {
			"422": {
				"labelId": "#/generators/label_id"
			},
			"200": {
				"labelId": "#/generators/label_id"
			}
		},
		"deleteLabel": {
			"422": {
				"labelId": "#/generators/label_id"
			},
			"204": {
				"labelId": "#/generators/label_id"
			}
		},
		"assignLabelToUsers": {
			"422": {
				"labelId": "#/generators/label_id",
				"userIds[*]": "../User/test_data_generation_configurations.json#/generators/user_id"
			},
			"204": {
				"labelId": "#/generators/label_id",
				"userIds[*]": "../User/test_data_generation_configurations.json#/generators/user_id"
			}
		},
		"updateLabel": {
			"422": {
				"labelId": "#/generators/label_id",
				"logoId": "#/generators/logo_id"
			},
			"200": {
				"labelId": "#/generators/label_id",
				"logoId": "#/generators/logo_id"
			}
		},
		"removeLabelsFromUsers": {
			"422": {
				"labelId": "#/generators/label_id",
				"userIds": "#/generators/remove_labels_users_ids"
			},
			"204": {
				"labelId": "#/generators/label_id",
				"userIds": "#/generators/remove_labels_users_ids"
			}
		}
	},
	"generators": {
		"label_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "Label.addLabel",
				"name": "label",
				"dataPath": "$.response.body:$.id"
			}
		],
		"remove_labels_users_ids": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.Label.assignLabelToUsers",
				"dataPath": "$.input.body:$.userIds",
				"params": {
					"labelId": "$label.response.body:$.id"
				}
			}
		],
		"logo_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.Upload.uploadFile",
				"dataPath": "$.response.body:$.id"
			}
		]
	}
}
