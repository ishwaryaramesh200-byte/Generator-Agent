{
  "apis": {
    "deleteComment": {
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id",
        "commentId": "#/generators/account_comment_id"
      }
    },
    "addAccountComment": {
      "422": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      },
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      }
    },
    "getComments": {
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id"
      }
    },
    "getComment": {
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id",
        "commentId": "#/generators/account_comment_id"
      }
    },
    "updateComment": {
      "200": {
        "accountId": "../Account/test_data_generation_configurations.json#/generators/account_id",
        "commentId": "#/generators/account_comment_id"
      }
    }
  },
  "generators": {
    "account_comment_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.AccountComments.addAccountComment",
        "dataPath": "$.response.body:$.id",
        "params": {
          "accountId": "$accounts.response.body:$.id"
        }
      }
    ]
  }
}