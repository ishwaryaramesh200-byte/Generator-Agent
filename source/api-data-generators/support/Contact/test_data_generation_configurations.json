  "generators": {
    "contact_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Contact.createContact",
        "dataPath": "$.response.body:$.id",
        "name": "contacts"
      }
    ],
    "contact_cf": [
      {
        "type": "remote",
        "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getCustomFieldSchema",
        "inputs": {
          "moduleName": "Contact"
        }
      }
    ],
    "contact_type": [
      {
        "type": "remote",
        "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
        "inputs": {
          "fieldName": "type",
          "moduleName": "Contact",
          "orgId": "$.input.query:$.orgId"
        }
      }
    ],
    "merge_contact_source_id": [
      {
        "type": "reference",
        "ref": "$.input.body:$.ids[0]"
      }
    ],
    "contact_custom_view_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getViews",
        "dataPath": "$.response.body:$.data[*].id",
        "params": {
          "module": "contacts"
        }
      }
    ] ,
    "contact_associate_account_ids" : [
      {
        "type" :"dynamic" ,
        "generatorOperationId" : "support.Contact.associateAccountsWithContact",
        "dataPath" : "$.response.body:$.results[?(@.success == true)].id" ,
        "params" : {
          "contactId" : "$contacts.response.body:$.id"
        }
      }
    ] ,
    "update_many_field_name": [
      {
        "type": "remote",
        "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
        "inputs": {
          "fieldName": "fieldName",
          "moduleName": "Contact",
          "orgId": "$.input.query:$.orgId"
        }
      }
    ] ,
    "spam_contact_id" : [
      {
        "type" : "dynamic" ,
        "generatorOperationId" : "support.Contact.markSpamContacts" ,
        "dataPath" : "$.response.body:$.results[?(@.success == true)].id",
        "params" : {
          "isSpam" : "true",
          "handleExistingTickets" : "true"
        }
      }
    ]
  }
