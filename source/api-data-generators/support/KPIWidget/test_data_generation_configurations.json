{
  "apis": {
    "getWidgetData": {
      "422": {
        "widgetId": "#/generators/kpi_widget_id"
      },
      "200": {
        "widgetId": "#/generators/kpi_widget_id"
      }
    }
  },
  "generators": {
    "kpi_widget_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.DashboardComponent.getAllDashboardComponents",
        "dataPath": "$.response.body:$.data[?((@.kpiWidgetDetails != null))].kpiWidgetDetails.widgetId"
      }
    ]
  }
}