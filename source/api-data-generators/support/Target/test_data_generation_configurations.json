{
  "apis": {
    "addTarget" : {
      "200" : {
        "targetTemplate" : "#/generators/target_template",
        "headers" : "#/generators/target_headers",
        "url" : "#/generators/target_url"
      } ,
      "422" : {
        "targetTemplate" : "#/generators/target_template" ,
        "headers" : "#/generators/target_headers",
        "url" : "#/generators/target_url"
      }
    },
    "invokeActionTarget": {
      "422": {
        "customActionId": "../CustomAction/test_data_generation_configurations.json#/generators/custom_action_id",
        "apiName": "#/generators/invokeActionTarget_apiName"
      },
      "200": {
        "customActionId": "../CustomAction/test_data_generation_configurations.json#/generators/custom_action_id",
        "apiName": "#/generators/invokeActionTarget_apiName"
      }
    },
    "updateTarget": {
      "422": {
        "targetId": "#/generators/target_id",
        "targetTemplate" : "#/generators/target_template",
        "headers" : "#/generators/target_headers",
        "url" : "#/generators/target_url"
      },
      "200": {
        "targetId": "#/generators/target_id",
        "targetTemplate" : "#/generators/target_template",
        "headers" : "#/generators/target_headers",
        "url" : "#/generators/target_url"
      }
    },
    "deleteTarget": {
      "422": {
        "targetId": "#/generators/target_id"
      },
      "204": {
        "targetId": "#/generators/target_id"
      }
    },
    "invokeTarget": {
      "422": {
        "targetId": "#/generators/target_id"
      },
      "200": {
        "targetId": "#/generators/target_id"
      }
    },
    "readTarget": {
      "200": {
        "targetId": "#/generators/target_id"
      }
    }
  },
  "generators": {
    "target_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Target.addTarget",
        "dataPath": "$.response.body:$.targetId"
      }
    ],
    "invokeActionTarget_apiName": [
      {
        "type": "remote",
        "generatorMethod": "targetAPIName",
        "inputs": {
          "orgId": "$.input.query:$.orgId"
        }
      }
    ] ,
    "target_template" : [
      {
        "type" : "static" ,
        "value" : [ "{\"accountName\":\"${accountName}\"}" ]
      }
    ] ,
    "target_headers" : [
      {
        "type" : "static" ,
        "value" : [ "{\"orgId\":\"${orgId}\"}"]
      }
    ] ,
    "target_url" : [
      {
        "type" : "static" ,
        "value" : [ "https://example.com/api/v1/accounts?orgId=${orgId}"]
      }
    ]
  }
}