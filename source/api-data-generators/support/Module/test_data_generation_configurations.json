{
  "apis": {
    "getModule": {
      "422": {
        "moduleId": "#/generators/custom_module_id"
      },
      "200": {
        "moduleId": "#/generators/custom_module_id"
      },
      "204": {
        "moduleId": "#/generators/custom_module_id"
      }
    },
    "editModule": {
      "422": {
        "moduleId": "#/generators/custom_module_id",
        "profileIds[*]": "../Profile/test_data_generation_configurations.json#/generators/profile_id",
        "departmentIds[*]": "../Department/test_data_generation_configurations.json#/generators/department_id"
      },
      "200": {
        "moduleId": "#/generators/custom_module_id",
        "profileIds[*]": "../Profile/test_data_generation_configurations.json#/generators/profile_id",
        "departmentIds[*]": "../Department/test_data_generation_configurations.json#/generators/department_id"
      },
      "204": {
        "moduleId": "#/generators/custom_module_id",
        "profileIds[*]": "../Profile/test_data_generation_configurations.json#/generators/profile_id",
        "departmentIds[*]": "../Department/test_data_generation_configurations.json#/generators/department_id"
      }
    },
    "createModule": {
      "422": {
        "profileIds[*]": "../Profile/test_data_generation_configurations.json#/generators/profile_id",
        "departmentIds[*]": "../Department/test_data_generation_configurations.json#/generators/department_id"
      },
      "200": {
        "profileIds[*]": "../Profile/test_data_generation_configurations.json#/generators/profile_id",
        "departmentIds[*]": "../Department/test_data_generation_configurations.json#/generators/department_id"
      }
    },
    "getCustomModulesCount": {
      "422": {
        "customModuleApiName": "#/generators/custom_module_api_name",
        "viewId": "#/generators/custom_module_view_id"
      },
      "200": {
        "customModuleApiName": "#/generators/custom_module_api_name",
        "viewId": "#/generators/custom_module_view_id"
      }
    },
    "getOrganizationModules": {
      "200": {
        "moduleAPIname[*]": "#/generators/module_api_name"
      },
      "204": {
        "moduleAPIname[*]": "#/generators/module_api_name"
      }
    }
  },
  "generators": {
    "custom_module_api_name": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Module.getModules",
        "dataPath": "$.response.body:$.data[?((@.isCustomModule == true))].apiKey",
        "params": {
          "featureFlags": "customModule"
        }
      }
    ],
    "custom_module_view_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getViews",
        "dataPath": "$.response.body:$.data[*].id",
        "params": {
          "module": "$.input.path:$.customModuleApiName"
        }
      }
    ],
    "module_api_name": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Module.getMyModules",
        "dataPath": "$.response.body:$.modules[*].apiKey"
      }
    ],
    "custom_module_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Module.getOrganizationModules",
        "dataPath": "$.response.body:$.data[?(@.isCustomModule == true)].id"
      }
    ]
  }
}