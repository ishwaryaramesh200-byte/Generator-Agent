	"generators": {
		"root_category_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.KBRootCategory.listAllRootCategory",
				"name": "rootCategory",
				"dataPath": "$.response.body:$.data[*].id"
			}
		],
		"create_new_root_category_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.KBRootCategory.addARootCategory",
				"dataPath": "$.response.body:$.id"
			}
		],
		"review_enabled_root_category_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.KBRootCategory.listAllRootCategory",
				"dataPath": "$.response.body:$.data[?( @.isReviewEnabled == true )].id"
			}
		],
		"logo_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.KbCategoryLogo.associateEntityAttachment",
				"dataPath": "$.response.body:$.id"
			}
		],
		"locale": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.KBRootCategory.getARootCategory",
				"dataPath": "$.response.body:$.translations[*].locale",
				"params": {
					"rootCategoryId": "$rootCategory.response.body:$.data[*].id"
				}
			}
		],
		"moderator_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.KBRootCategory.addModerators",
				"dataPath": "$.input.body:$.moderatorIds[*]"
			}
		]
