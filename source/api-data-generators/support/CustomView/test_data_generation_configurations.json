  "generators": {
    "myQueueSlots": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getMyQueueSlots",
        "dataPath": "$.response.body:$.queueSlots[*]",
        "params": {
          "mode": "$.input.query:$.mode",
          "departmentId": "$.input.query:$.departmentId"
        }
      }
    ],
    "getExtensionFields": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Field.getExtensionFields",
        "dataPath": "$.response.body:$.data[*].apiName",
        "params": {
          "module": "tickets"
        }
      }
    ],
    "get_customView_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getViews",
        "dataPath": "$.response.body:$.data[*].id",
        "name" : "custom_views",
        "params": {
          "departmentId": "../Department/test_data_generation_configurations.json#/generators/department_id"
        }
      }
    ],
    "start_view_department_id" : [
      {
        "type" : "reference" ,
        "ref" : "$custom_views.input.query:$.departmentId"
      }
    ],
    "get_user_customView_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getViews",
        "dataPath": "$.response.body:$.data[?(@.isCustomView == true)].id",
        "params": {
          "departmentId": "../Department/test_data_generation_configurations.json#/generators/department_id"
        }
      }
    ],
     "getTicket_Module_ViewId": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getViews",
        "dataPath": "$.response.body:$.data[?(@.isCustomView == true)].id",
        "params": {
          "departmentId": "../Department/test_data_generation_configurations.json#/generators/department_id",
          "module": "tickets"
        }
      }
    ],
    "add_customView": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.addCustomView",
        "dataPath": "$.response.body:$.id"
      }
    ],
    "addCustomView_[*]": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.SavedFilters.createSavedFilter",
        "dataPath": "$.response.body:$.data.criteria.criteriaFields[*].value"
      }
    ],
    "addCustomView_pattern": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.ArchivedView.getArchivedCustomView",
        "dataPath": "$.response.body:$.criteria.pattern"
      }
    ],
    "updateSelectedFields_getViews_module": [
      {
        "type": "static",
        "value": [
          "tickets"
        ]
      }
    ],
    "updateArchivedViewPreference_selectedFields": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getArchivedViewPreference",
        "dataPath": "$.response.body:$.data.fields" ,
        "params" : {
          "module" : "$.input.query:$.module" ,
          "departmentId" : "$.input.query:$.departmentId"
        }
      }
    ],
    "collect_selectedFields": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getSelectedFields",
        "dataPath": "$.response.body:$.fields",
        "params": {
          "cvId": "$.input.path:$.cvId"
        }
      }
    ],
    "addCustomView_condition": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Criteria.getCustomViewCriteria",
        "dataPath": "$.response.body:$.criteriaFields[0].fieldDetails[0].availableConditions[0].condition",
        "params": {
          "module": "$.input.body:$.module",
          "departmentId": "$.input.body:$.department"
        }
      }
    ],
    "updateSelectedFields_cvId": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getViews",
        "dataPath": "$.response.body:$.data[*].id",
        "name": "view",
        "params": {
          "module": "$generators:#/generators/updateSelectedFields_getViews_module"
        }
      }
    ],
    "updateCustomView_condition": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Criteria.getCustomViewCriteria",
        "dataPath": "$.response.body:$.criteriaFields[*].fieldDetails[*].availableConditions[*].condition"
      }
    ],
    "updateCustomView_fieldName": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Field.massUpdateFields",
        "dataPath": "$.response.body:$.data[*].name"
      }
    ],
    "updateSelectedFields_departmentId": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Department.getSyncDepartments",
        "dataPath": "$.response.body:$.data[*].id"
      }
    ],
    "queueSlots_items[*]": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomView.getQueueSlots",
        "dataPath": "$.response.body:$.queueSlots",
        "params": {
          "mode": "$.input.query:$.mode"
        }
      }
    ],
    "queueSlots_dueInMinutes": [
      {
        "type": "static",
        "value": [
          [
            "overDue",
            "30",
            "60",
            "noDue",
            "onhold"
          ],
          [
            "overDue",
            "15",
            "120",
            "720",
            "2880",
            "onhold"
          ]
        ]
      }
    ],
    "queueSlots[*]": [
      {
        "type": "static",
        "value": [
          [
            "customers",
            "potentials",
            "contacts_leads",
            "unmapped"
          ]
        ]
      }
    ],
    "module": [
      {
        "type": "static",
        "value": [
          "tickets"
        ]
      }
    ],
    "apiName": [
      {
        "type": "static",
        "value": [
          "tickets"
        ]
      }
    ],
    "criteria": [
      {
        "type": "static",
        "value": [
          {
            "criteriaFields": [
              {
                "field": {
                  "module": "tickets",
                  "apiName": "contactId",
                  "fieldLabel": "Contact Name"
                },
                "condition": "is not empty",
                "value": {
                  "type": "primitive"
                }
              }
            ],
            "criteriaPattern": "1"
          }
        ]
      }
    ],
    "queueSlots_items_with_condition_based": [
      {
        "type": "conditional",
        "conditions": [
          {
            "when": {
              "key": "$.input.query:$.mode",
              "equals": "crmContactType"
            },
            "then": {
              "use": "$generators:#/generators/queueSlots_items[*]"
            }
          },
          {
            "when": {
              "key": "$.input.query:$.mode",
              "equals": "priority"
            },
            "then": {
              "use": "$generators:#/generators/queueSlots_items[*]"
            }
          },
          {
            "when": {
              "key": "$.input.query:$.mode",
              "equals": "status"
            },
            "then": {
              "use": "$generators:#/generators/queueSlots_items[*]"
            }
          },
          {
            "when": {
              "key": "$.input.query:$.mode",
              "equals": "dueInMinutes"
            },
            "then": {
              "use": "$generators:#/generators/queueSlots_dueInMinutes"
            }
          }
        ],
        "else": "$generators:#/generators/queueSlots[*]"
      }
    ]
  }
}
