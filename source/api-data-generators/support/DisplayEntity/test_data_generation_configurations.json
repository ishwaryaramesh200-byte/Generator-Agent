{
  "apis": {
    "displayEntity": {
      "200": {
        "agentId": "../Agent/test_data_generation_configurations.json#/generators/agent_id",
        "entityId": "#/generators/entity_id"
      },
      "204": {
        "agentId": "../Agent/test_data_generation_configurations.json#/generators/agent_id",
        "entityId": "#/generators/entity_id"
      },
      "422": {
        "agentId": "../Agent/test_data_generation_configurations.json#/generators/agent_id",
        "entityId": "#/generators/entity_id"
      }
    }
  },
  "generators": {
    "entity_id": [
      {
        "type": "conditional",
        "conditions": [
          {
            "when": {
              "key": "$.input.path:$.entityName",
              "equals": "tickets"
            },
            "then": {
              "use": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
            }
          },
          {
            "when": {
              "key": "$.input.path:$.entityName",
              "equals": "contacts"
            },
            "then": {
              "use": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
            }
          }
        ]
      }
    ]
  }
}