{
  "apis": {
    "readCustomAction": {
      "200": {
        "customActionId": "#/generators/custom_action_id"
      }
    },
    "readCustomActionByIds": {
      "422": {
        "customActionIds[*]": "#/generators/custom_action_id"
      },
      "200": {
        "customActionIds[*]": "#/generators/custom_action_id"
      }
    },
    "deleteCustomAction": {
      "422": {
        "customActionId": "#/generators/custom_action_id"
      },
      "204": {
        "customActionId": "#/generators/custom_action_id"
      }
    },
    "updateCustomAction": {
      "422": {
        "customActionId": "#/generators/custom_action_id",
        "targetId": "../Target/test_data_generation_configurations.json#/generators/target_id",
        "secondaryTargetIds[*]": "../Target/test_data_generation_configurations.json#/generators/target_id"
      },
      "200": {
        "customActionId": "#/generators/custom_action_id",
        "targetId": "../Target/test_data_generation_configurations.json#/generators/target_id",
        "secondaryTargetIds[*]": "../Target/test_data_generation_configurations.json#/generators/target_id"
      }
    },
    "addCustomAction": {
      "422": {
        "targetId": "../Target/test_data_generation_configurations.json#/generators/target_id",
        "secondaryTargetIds[*]": "../Target/test_data_generation_configurations.json#/generators/target_id"
      },
      "200": {
        "targetId": "../Target/test_data_generation_configurations.json#/generators/target_id",
        "secondaryTargetIds[*]": "../Target/test_data_generation_configurations.json#/generators/target_id"
      }
    }
  },
  "generators": {
    "custom_action_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.CustomAction.addCustomAction",
        "dataPath": "$.response.body:$.customActionId"
      }
    ]
  }
}