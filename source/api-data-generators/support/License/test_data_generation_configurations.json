{
	"apis": {
		"featureInfo": {
			"200": {
				"featureCodes[*]": "#/generators/featureCode_Value"
			}
		},
		"checkLicenseAvailability": {
			"200": {
				"featureCode": "#/generators/featureCode_Value",
				"entityId": "#/generators/conditional_entityId"
			}
		}
	},
	"generators": {
		"featureCode_Value": [
			{
				"type": "static",
				"value": [
					"AGNT01",
					"DEPT01",
					"DOMN01",
					"TEAM01"
				]
			}
		],
		"conditional_entityId": [
			{
				"type": "conditional",
				"conditions": [
					{
						"when": {
							"key": "$.input.path:$.featureCode",
							"equals": "TEAM01"
						},
						"then": {
							"use": "$generators:#/generators/getEntityId_departmentId"
						}
					}
				],
				"else": "$generators:#/generators/null_value"
			}
		],
		"null_value": [
			{
				"type": "static",
				"value": [
					""
				]
			}
		],
		"getEntityId_departmentId": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.Department.getSyncDepartments",
				"dataPath": "$.response.body:$.data[*].id"
			}
		]
	}
}
