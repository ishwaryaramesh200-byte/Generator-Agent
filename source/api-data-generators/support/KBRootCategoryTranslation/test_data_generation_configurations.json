{
	"apis": {
		"editARootCategoryTranslation": {
			"422": {
				"locale": "#/generators/locale",
				"rootCategoryId": "../KBRootCategory/test_data_generation_configurations.json#/generators/root_category_id"
			},
			"200": {
				"locale": "#/generators/locale",
				"rootCategoryId": "../KBRootCategory/test_data_generation_configurations.json#/generators/root_category_id"
			}
		},
		"addARootCategoryTranslation": {
			"422": {
				"rootCategoryId": "../KBRootCategory/test_data_generation_configurations.json#/generators/root_category_id"
			},
			"200": {
				"rootCategoryId": "../KBRootCategory/test_data_generation_configurations.json#/generators/root_category_id"
			}
		}
	},
	"generators": {
		"locale": [
			{
				"type": "reference",
				"ref": "$rootCategory.response.body:$.data[*].translations[*].locale"
			}
		]
	}
}
