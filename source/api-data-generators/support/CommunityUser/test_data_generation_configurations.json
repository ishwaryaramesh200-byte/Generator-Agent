{
  "apis" : {
    "updateCommunityUserInfo" : {
      "422" : {
        "isModerated" : "#/generators/updateCommunityUserInfo_isModerated",
        "userId" : "#/generators/getUserId"
      },
      "204" : {
        "isModerated" : "#/generators/updateCommunityUserInfo_isModerated",
        "userId" : "#/generators/getUserId"
      }
    },
    "listAllCommunityUserFollowers" : {
      "422" : {
        "userId" : "#/generators/getUserId"
      },
      "200" : {
        "userId" : "#/generators/getUserId"
      }
    },
    "unmoderateUsers" : {
      "422" : {
        "userIds[*]" : "#/generators/unmoderateUsers_[*]"
      },
      "200" : {
        "userIds[*]" : "#/generators/unmoderateUsers_[*]"
      }
    },
    "getUserInfo" : {
      "200" : {
        "userId" : "#/generators/getUserId"
      }
    },
    "followAUser" : {
      "204" : {
        "userId" : "../User/test_data_generation_configurations.json#/generators/user_id"
      }
    },
    "unfollowAUser" : {
      "204" : {
        "userId" : "../User/test_data_generation_configurations.json#/generators/user_id"
      }
    }
  },
  "generators" : {
    "updateCommunityUserInfo_isModerated" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "support.CommunityUser.getUserInfo",
      "dataPath" : "$.response.body:$.isModerated"
    } ],
    "unmoderateUsers_[*]" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "support.CommunityUser.listAllCommunityModeratedUsers",
      "dataPath" : "$.response.body:$.data[*].id"
    } ],
    "getUserId" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "support.CommunityTopic.listAllTopics",
      "dataPath" : "$.response.body:$.data[*].creator.id",
      "params" : { }
    } ]
  }
}