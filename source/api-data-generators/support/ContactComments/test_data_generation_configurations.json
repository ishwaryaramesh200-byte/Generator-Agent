{
  "apis": {
    "addContactComment": {
      "422": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      },
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      }
    },
    "deleteComment": {
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id",
        "commentId": "#/generators/contact_comment_id"
      }
    },
    "getComments": {
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      }
    },
    "getComment": {
      "200": {
        "commentId": "#/generators/contact_comment_id",
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      }
    },
    "updateComment": {
      "200": {
        "commentId": "#/generators/contact_comment_id",
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      }
    }
  },
  "generators": {
    "contact_comment_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.ContactComments.addContactComment",
        "dataPath": "$.response.body:$.id",
        "params": {
          "contactId": "$contacts.response.body:$.id"
        }
      }
    ]
  }
}