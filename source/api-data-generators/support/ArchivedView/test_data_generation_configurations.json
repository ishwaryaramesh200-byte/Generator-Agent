{
	"apis": {
		"archiveCustomView": {
			"204": {
				"cvId": "#/generators/custom_view_id"
			}
		},
		"getArchivedCustomView": {
			"422": {
				"cvId": "#/generators/mark_archived_custom_view_id"
			},
			"200": {
				"cvId": "#/generators/mark_archived_custom_view_id"
			}
		},
		"deleteArchivedView": {
			"204": {
				"cvId": "#/generators/mark_archived_custom_view_id"
			}
		},
		"markUnarchive": {
			"204": {
				"cvId": "#/generators/mark_archived_custom_view_id"
			}
		},
		"getArchivedViews": {
			"422": {
				"departmentId": "../../Department/test_data_generation_configurations.json#/generators/department_id"
			},
			"200": {
				"departmentId": "../../Department/test_data_generation_configurations.json#/generators/department_id"
			}
		}
	},
	"generators": {
		"custom_view_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.CustomView.getViews",
				"dataPath": "$.response.body:$.data[?((@.isCustomView == true))].id" ,
				"params" : {
					"departmentId" : "../../Department/test_data_generation_configurations.json#/generators/department_id",
					"module" : "tickets"
				}
			}
		],
		"archived_custom_view_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.ArchivedView.getArchivedViews",
				"dataPath": "$.response.body:$.data[0].id",
				"params": {
					"departmentId" : "../../Department/test_data_generation_configurations.json#/generators/department_id",
					"module": "tickets"
				}
			}
		],
		"mark_archived_custom_view_id": [
			{
				"type": "dynamic",
				"generatorOperationId": "support.ArchivedView.archiveCustomView",
				"dataPath": "$.input.path:$.cvId"
			}
		]
	}
}
