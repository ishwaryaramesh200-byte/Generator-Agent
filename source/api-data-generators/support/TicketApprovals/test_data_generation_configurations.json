{
  "apis": {
    "listApprovals": {
      "404": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "422": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "200": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      }
    },
    "getApproval": {
      "404": {
        "approvalId": "#/generators/approval_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "200": {
        "approvalId": "#/generators/approval_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      }
    },
    "addApproval": {
      "404": {
        "approverIds[*]": "../Agent/test_data_generation_configurations.json#/generators/agent_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "422": {
        "approverIds[*]": "../Agent/test_data_generation_configurations.json#/generators/agent_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "200": {
        "approverIds[*]": "../Agent/test_data_generation_configurations.json#/generators/agent_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      }
    },
    "updateApproval": {
      "404": {
        "approvalId": "#/generators/approval_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "422": {
        "approvalId": "#/generators/approval_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "200": {
        "approvalId": "#/generators/approval_id",
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      }
    }
  },
  "generators": {
    "approval_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.TicketApprovals.addApproval",
        "dataPath": "$.response.body:$.approvals[*].id" ,
        "name": "ticket_approval",
        "params" : {
          "ticketId" : "$tickets.response.body:$.id"
        }
      }
    ]
  }
}