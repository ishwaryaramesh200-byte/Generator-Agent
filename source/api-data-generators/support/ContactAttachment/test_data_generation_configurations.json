{
  "apis": {
    "getAttachments": {
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      },
      "204": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      }
    },
    "viewContactAttachment": {
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id",
        "attachmentId": "#/generators/contact_attachment_id"
      }
    },
    "createAttachment": {
      "422": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      },
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id"
      }
    },
    "deleteAttachment": {
      "200": {
        "contactId": "../Contact/test_data_generation_configurations.json#/generators/contact_id",
        "attachmentId": "#/generators/contact_attachment_id"
      }
    }
  },
  "generators": {
    "contact_attachment_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.ContactAttachment.createAttachment",
        "dataPath": "$.response.body:$.id",
        "params": {
          "contactId": "$contacts.response.body:$.id"
        }
      }
    ]
  }
}