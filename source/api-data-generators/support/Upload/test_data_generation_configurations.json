{
  "apis": {
    "deleteTemporaryAttachment": {
      "200": {
        "attachmentId": "#/generators/upload_id"
      }
    },
    "viewTemporaryAttachment": {
      "200": {
        "uploadId": "#/generators/upload_id"
      }
    },
    "getInlineAttachment": {
      "422": {
        "storeId": "#/generators/inline_attachment_store_id",
        "id": "#/generators/inline_attachment_id"
      },
      "200": {
        "storeId": "#/generators/inline_attachment_store_id",
        "id": "#/generators/inline_attachment_id"
      }
    }
  },
  "generators": {
    "upload_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Upload.uploadFile",
        "dataPath": "$.response.body:$.id",
        "name": "uploads",
        "params": {
          "type": "Attachment"
        }
      }
    ],
    "inline_attachment_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Upload.uploadFile",
        "dataPath": "$.response.body:$.id",
        "name": "inline_attachments",
        "params": {
          "type": "InlineAttachment"
        }
      }
    ],
    "inline_attachment_store_id": [
      {
        "type": "reference",
        "ref": "$inline_attachments.response.body:$.storeId"
      }
    ]
  }
}