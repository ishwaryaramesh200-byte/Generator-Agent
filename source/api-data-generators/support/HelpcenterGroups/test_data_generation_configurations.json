{
  "apis" : {
    "getUsers" : {
      "200" : {
        "groupId" : "#/generators/group_id"
      }
    },
    "getGroup" : {
      "200" : {
        "groupId" : "#/generators/group_id"
      }
    },
    "removeUsers" : {
      "422" : {
        "userIds[*]" : "#/generators/removeUsers_id",
        "groupId" : "#/generators/group_id"
      },
      "204" : {
        "userIds[*]" : "#/generators/removeUsers_id",
        "groupId" : "#/generators/group_id"
      }
    },
    "updateGroup" : {
      "422" : {
        "logoId" : "#/generators/logo_id",
        "groupId" : "#/generators/group_id"
      },
      "200" : {
        "logoId" : "#/generators/logo_id",
        "groupId" : "#/generators/group_id"
      }
    },
    "deleteGroup" : {
      "422" : {
        "groupId" : "#/generators/group_id"
      },
      "204" : {
        "groupId" : "#/generators/group_id"
      }
    },
    "addGroup" : {
      "422" : {
        "helpCenterId" : "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id",
        "logoId" : "#/generators/logo_id"
      },
      "200" : {
        "helpCenterId" : "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id",
        "logoId" : "#/generators/logo_id"
      }
    },
    "getGroups" : {
      "200" : {
        "helpCenterId" : "../Helpcenter/test_data_generation_configurations.json#/generators/help_center_id"
      }
    },
    "addUsers" : {
      "422" : {
        "users[*].id" : "../User/test_data_generation_configurations.json#/generators/user_id",
        "groupIds[*]" : "#/generators/group_id",
        "labelIds[*]" : "../Label/test_data_generation_configurations.json#/generators/label_id",
        "groupId" : "#/generators/group_id"
      },
      "204" : {
        "users[*].id" : "../User/test_data_generation_configurations.json#/generators/user_id",
        "groupIds[*]" : "#/generators/group_id",
        "labelIds[*]" : "../Label/test_data_generation_configurations.json#/generators/label_id",
        "groupId" : "#/generators/group_id"
      }
    }
  },
  "generators" : {
    "logo_id" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "HelpcenterUpload.uploadFile",
      "dataPath" : "$.response.body:$.id"
    } ],
    "group_id" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "HelpcenterGroups.addGroup",
      "name" : "groups",
      "dataPath" : "$.response.body:$.id"
    } ],
    "removeUsers_id" : [ {
      "type" : "dynamic",
      "generatorOperationId" : "HelpcenterGroups.addUsers",
      "dataPath" : "$.input.body:$.users[*].id",
      "params" : {
        "groupIds[*]" : "$groups.response.body:$.id",
        "groupId" : "$groups.response.body:$.id"
      }
    } ]
  }
}