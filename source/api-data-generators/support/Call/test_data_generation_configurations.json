  "generators": {
    "call_cf": [
      {
        "type": "remote",
        "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getCustomFieldSchema",
        "inputs": {
          "moduleName": "Call"
        }
      }
    ],
    "call_priority": [
      {
        "type": "remote",
        "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
        "inputs": {
          "fieldName": "priority",
          "moduleName": "Call",
          "orgId": "$.input.query:$.orgId"
        }
      }
    ],
    "call_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Call.addCall",
        "dataPath": "$.response.body:$.id" ,
        "name" : "calls" ,
        "params" : {
          "status" : "Completed" ,
          "startTime" : "../Common/test_data_generation_configurations.json#/generators/past_time"
        }
      }
    ],
    "updateMany_fieldName": [
      {
        "type": "remote",
        "generatorMethod": "applicationDriver.rpc.desk.DynamicDataProvider.getDynamicEnumValues",
        "inputs": {
          "fieldName": "fieldName",
          "moduleName": "Call",
          "orgId": "$.input.query:$.orgId"
        }
      }
    ] ,
    "spam_call_id" : [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Call.addCall",
        "dataPath": "$.response.body:$.id"
      }
    ] ,
    "call_start_time": [
      {
        "type" : "conditional" ,
        "conditions" : [
          {
             "when" : {
               "key" : "$calls.input.body:$.status" ,
               "equals" : "Scheduled"
             } ,
            "then" : {
              "use" : "../Common/test_data_generation_configurations.json#/generators/future_time"
            }
          } ,
          {
            "when" : {
              "key" : "$calls.input.body:$.status" ,
              "equals" : "Completed"
            } ,
            "then" : {
              "use" : "../Common/test_data_generation_configurations.json#/generators/past_time"
            }
          }
        ] ,
        "else" : "../Common/test_data_generation_configurations.json#/generators/current_time"
      }
    ] ,
    "call_direction" : [
      {
        "type" : "conditional" ,
        "conditions" : [
          {
            "when" : {
              "key" : "$calls.input.body:$.status" ,
              "equals" : "Scheduled"
            } ,
            "then" : {
              "use" : "#/generators/call_direction_in"
            }
          }
        ] ,
        "else" : "#/generators/call_direction_out"
      }
    ] ,
    "call_direction_in" : [
      {
        "type" : "static" ,
        "value" : ["inbound"]
      }
    ] ,
    "call_direction_out" : [
      {
        "type" : "static" ,
        "value" : ["outbound"]
      }
    ]
  }
}