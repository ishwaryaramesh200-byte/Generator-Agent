{
  "apis": {
    "updateRole": {
      "422": {
        "shareDataWithPeers": "#/generators/role_shareDataWithPeers",
        "roleId": "#/generators/role_id"
      },
      "200": {
        "shareDataWithPeers": "#/generators/role_shareDataWithPeers",
        "roleId": "#/generators/role_id"
      }
    },
    "getRolesByIds": {
      "422": {
        "roleIds[*]": "#/generators/role_id"
      },
      "200": {
        "roleIds[*]": "#/generators/role_id"
      },
      "204": {
        "roleIds[*]": "#/generators/role_id"
      }
    },
    "getRole": {
      "200": {
        "roleId": "#/generators/role_id"
      }
    },
    "getAgentsByRole": {
      "200": {
        "roleId": "#/generators/role_id"
      },
      "204": {
        "roleId": "#/generators/role_id"
      }
    },
    "deleteRole": {
      "422": {
        "transferToRoleId": "#/generators/role_id",
        "roleId": "#/generators/create_role_id"
      },
      "204": {
        "transferToRoleId": "#/generators/role_id",
        "roleId": "#/generators/create_role_id"
      }
    },
    "addRole": {
      "422": {
        "shareDataWithPeers": "#/generators/role_shareDataWithPeers",
        "reportsTo": "#/generators/role_id"
      },
      "200": {
        "shareDataWithPeers": "#/generators/role_shareDataWithPeers",
        "reportsTo": "#/generators/role_id"
      }
    }
  },
  "generators": {
    "role_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Role.getRoles",
        "dataPath": "$.response.body:$.data[*].id",
        "params": {
          "isVisible": "true"
        }
      }
    ],
    "role_shareDataWithPeers": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Role.getRoles",
        "dataPath": "$.response.body:$.data[*].shareDataWithPeers"
      }
    ],
    "create_role_id" : [
      {
        "type": "dynamic",
        "generatorOperationId": "support.Role.addRole",
        "dataPath": "$.response.body:$.id"
      }
    ]
  }
}