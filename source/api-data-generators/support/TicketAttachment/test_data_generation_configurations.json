{
  "apis": {
    "getAttachments": {
      "200": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "204": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      }
    },
    "viewTicketAttachment": {
      "200": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id",
        "attachmentId": "#/generators/ticket_attachment_id"
      }
    },
    "createAttachment": {
      "422": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      },
      "200": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id"
      }
    },
    "deleteAttachment": {
      "404": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id",
        "attachmentId": "#/generators/ticket_attachment_id"
      },
      "200": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id",
        "attachmentId": "#/generators/ticket_attachment_id"
      }
    },
    "updateAttachment": {
      "422": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id",
        "attachmentId": "#/generators/ticket_attachment_id"
      },
      "200": {
        "ticketId": "../Ticket/test_data_generation_configurations.json#/generators/ticket_id",
        "attachmentId": "#/generators/ticket_attachment_id"
      }
    }
  },
  "generators": {
    "ticket_attachment_id": [
      {
        "type": "dynamic",
        "generatorOperationId": "support.TicketAttachment.createAttachment",
        "dataPath": "$.response.body:$.id",
        "params": {
          "ticketId": "$tickets.response.body:$.id"
        }
      }
    ]
  }
}