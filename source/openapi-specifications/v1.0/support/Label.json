  "components" : {
    "schemas" : {
      "labelPreferenceResponse" : {
        "type" : [ "null", "object" ],
        "additionalProperties" : false,
        "properties" : {
          "bgColor" : {
            "type" : [ "string", "null" ],
            "description" : "background color of the label",
            "maxLength" : 100,
            "minLength" : 0,
            "pattern" : "^#?(([a-fA-F0-9]){3}){1,2}$"
          },
          "shape" : {
            "type" : [ "string", "null" ],
            "description" : "shape of the label",
            "enum" : [ "ROUNDED", "RECTANGLE" ],
            "maxLength" : 100,
            "minLength" : 0
          },
          "textColor" : {
            "type" : [ "string", "null" ],
            "description" : "text color of the label",
            "maxLength" : 100,
            "minLength" : 0,
            "pattern" : "^#?(([a-fA-F0-9]){3}){1,2}$"
          }
        },
        "required" : [ "bgColor", "shape", "textColor" ]
      },
      "labelMetaResponse" : {
        "type" : [ "null", "object" ],
        "additionalProperties" : false,
        "properties" : {
          "preferences" : {
            "$ref" : "./Label.json#/components/schemas/labelPreferenceResponse"
          },
          "name" : {
            "type" : [ "string", "null" ],
            "maxLength" : 100,
            "minLength" : 0,
            "pattern" : "[0-9a-zA-Z_\\-\\&\\.\\$\\@\\^\\(\\)\\{\\}\\~\\=\\-\\|\\*\\+\\?\\[\\]\\,\\:\\'\\/\\!\\P{InBasicLatin}\\s]+"
          },
          "id" : {
            "type" : [ "string", "null", "integer" ],
            "format" : "int64",
            "pattern" : "([0-9]+)"
          },
          "logoUrl" : {
            "type" : [ "string", "null" ],
            "maxLength" : 200,
            "minLength" : 0,
            "pattern" : "(^(ht|f)tp(s?)\\://[-.\\w]*(/?)([a-zA-Z0-9\\-\\.\\?\\,\\:\\'/\\\\+=&amp;%\\$#_@]*)?$)"
          }
        },
        "required" : [ "id", "logoUrl", "name", "preferences" ]
      },
      "creatorResponse" : {
        "type" : [ "null", "object" ],
        "additionalProperties" : false,
        "properties" : {
          "photoUrl" : {
            "type" : [ "string", "null" ],
            "maxLength" : 200,
            "minLength" : 0,
            "pattern" : "(^(ht|f)tp(s?)\\://[-.\\w]*(/?)([a-zA-Z0-9\\-\\.\\?\\,\\:\\'/\\\\+=&amp;%\\$#_@]*)?$)"
          },
          "name" : {
            "type" : [ "string", "null" ],
            "maxLength" : 100,
            "minLength" : 0
          },
          "id" : {
            "type" : [ "string", "null", "integer" ],
            "format" : "int64",
            "pattern" : "([0-9]+)"
          }
        },
        "required" : [ "id", "name", "photoUrl" ]
      },
      "userLabelResponse" : {
        "additionalProperties" : false,
        "allOf" : [ {
          "$ref" : "./User.json#/components/schemas/userMetaJson"
        }, {
          "type" : [ "null", "object" ],
          "properties" : {
            "assignee" : {
              "$ref" : "./Label.json#/components/schemas/creatorResponse"
            },
            "assignedTime" : {
              "type" : [ "string", "null" ],
              "maxLength" : 100,
              "minLength" : 0,
              "pattern" : "(((19|20)\\d\\d)[-/.]{1}(0?[1-9]|1[012])[-/.]{1}(0?[1-9]|[12][0-9]|3[01])(T)([01]?[0-9]|2[0-3]):[0-5]?[0-9]:[0-5]?[0-9]([/.][\\d]{0,3})?(Z))"
            }
          },
          "required" : [ "assignedTime", "assignee" ]
        } ]
      },
      "userLabelData" : {
        "type" : [ "null", "array" ],
        "items" : {
          "$ref" : "./Label.json#/components/schemas/userLabelResponse"
        }
      },
      "updateLabelPayloadJSON" : {
        "type" : [ "null", "object" ],
        "additionalProperties" : false,
        "properties" : {
          "preferences" : {
            "$ref" : "./Label.json#/components/schemas/labelPreferenceResponse"
          },
          "name" : {
            "type" : [ "string", "null" ],
            "description" : "Name of the label",
            "maxLength" : 100,
            "minLength" : 0,
            "pattern" : "[0-9a-zA-Z_\\-\\&\\.\\$\\@\\^\\(\\)\\{\\}\\~\\=\\-\\|\\*\\+\\?\\[\\]\\,\\:\\'\\/\\!\\P{InBasicLatin}\\s]+"
          },
          "logoId" : {
